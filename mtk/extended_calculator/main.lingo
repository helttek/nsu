S = negative | sum | sub | mul | div | int | var;
sum = "(" ws S:l ws "+" ws S:r ws ")" { ArSum(:l, :r)};
mul = "(" ws S:l ws "*" ws S:r ws ")" { ArMult(:l, :r)};
div = "(" ws S:l ws "/" ws S:r ws ")" { ArDiv(:l, :r)};
sub = "(" ws S:l ws "-" ws S:r ws ")" { ArSub(:l, :r)};
negative = ws "-" ws S:l ws {ArNeg(:l)};

var = (letter+digit*)+ $v{ArVar($v)};
int = digit+ $d{ArInt(s2i($d))};
letter = 'a'-'z' | 'A'-'Z';
digit = '0'-'9';
ws = (' ' | '\t' | '\n' | '\r')*;
